{
  "trailingComma": "all",
  "useTabs": false,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": false,
  "printWidth": 90,
  "arrowParens": "always",
  "endOfLine": "lf",
  "editorconfig": true,
  "singleAttributePerLine": true,
  "plugins": [
    "prettier-plugin-organize-imports"
  ],
  "organizeImportsSkipDestructiveCodeActions": true
}
